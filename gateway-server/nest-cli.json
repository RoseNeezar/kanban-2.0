{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway-server/src",
  "monorepo": true,
  "root": "apps/gateway-server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway-server/tsconfig.app.json"
  },
  "projects": {
    "gateway-server": {
      "type": "application",
      "root": "apps/gateway-server",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-server/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "task-service": {
      "type": "application",
      "root": "apps/task-service",
      "entryFile": "main",
      "sourceRoot": "apps/task-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task-service/tsconfig.app.json"
      }
    },
    "shared-interface": {
      "type": "library",
      "root": "libs/shared-interface",
      "entryFile": "index",
      "sourceRoot": "libs/shared-interface/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared-interface/tsconfig.lib.json"
      }
    }
  }
}